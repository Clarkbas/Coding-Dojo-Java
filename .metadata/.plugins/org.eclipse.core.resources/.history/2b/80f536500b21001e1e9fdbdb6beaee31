package com.Coding.mostrarFecha.homeController;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
@Controller
public class HomeController {
    @RequestMapping("/")
    public String date() {
        return "/date.html";
    }
    
    @RequestMapping("/")
    public String home() {
        return "date.html";
    }
}




